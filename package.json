{
    "name": "prisma-appsync",
    "description": "⚡ AppSync GraphQL API Generator for ◭ Prisma 2, the next-gen ORM.",
    "version": "1.0.0-beta.51",
    "bin": "./dist/generator.js",
    "repository": "git@github.com:maoosi/prisma-appsync.git",
    "author": "maoosi <hello@sylvainsimao.fr>",
    "license": "BSD-2-Clause",
    "scripts": {
        "prebuild": "rm -rf dist && rm -rf test/generated",
        "build": "yarn build:generator && yarn build:client",
        "build:generator": "esbuild src/generator.ts --bundle --platform=node --external:fsevents --outfile=dist/generator.js",
        "build:client": "esbuild src/client/index.ts --bundle --minify --keep-names --platform=node --external:fsevents --external:@prisma/client --outfile=dist/prisma-appsync/index.js && tsc src/client/*.ts --outDir dist/prisma-appsync/ --declaration --emitDeclarationOnly",
        "postbuild": "cp -R src/generator/templates dist/templates && chmod -R 755 dist",
        "docs:dev": "vuepress dev docs --host localhost",
        "docs:build": "vuepress build docs",
        "pretest": "yarn build && prisma generate --schema=./test/schema.prisma",
        "test": "jest test/ -e",
        "prepare": "yarn test"
    },
    "keywords": [
        "api",
        "appsync",
        "aws",
        "crud",
        "generator",
        "graphql",
        "prisma"
    ],
    "devDependencies": {
        "@casl/ability": "^4.1.6",
        "@maoosi/appsync-schema-converter": "^1.0.2",
        "@prisma/client": "^2.20.1",
        "@prisma/generator-helper": "^2.20.1",
        "@prisma/sdk": "^2.20.1",
        "@types/dot-object": "^2.1.2",
        "@types/fs-extra": "^8.1.0",
        "@types/jest": "^26.0.10",
        "@types/js-yaml": "^4.0.0",
        "@types/lodash": "^4.14.160",
        "@types/nunjucks": "^3.1.3",
        "@types/pluralize": "^0.0.29",
        "@types/prettier": "^2.0.0",
        "@types/validator": "^13.1.0",
        "@vuepress/plugin-back-to-top": "^1.8.2",
        "@vuepress/plugin-medium-zoom": "^1.8.2",
        "babel-jest": "^26.6.1",
        "dot-object": "^2.1.4",
        "esbuild": "^0.8.33",
        "fs-extra": "^9.0.0",
        "graphql": "^15.3.0",
        "graphql-annotations": "^0.0.3",
        "jest": "^26.4.2",
        "jest-transform-graphql": "^2.1.0",
        "js-yaml": "^3.14.1",
        "nunjucks": "^3.2.1",
        "pluralize": "^8.0.0",
        "prettier": "^2.0.5",
        "prisma": "^2.20.1",
        "ts-custom-error": "^3.2.0",
        "ts-jest": "^26.3.0",
        "typescript": "^4.1.3",
        "validator": "^13.1.17",
        "vuepress": "^1.8.2",
        "xss": "^1.0.8"
    }
}
